<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="refactor_btn">
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginBottom">20dp</item>
        <item name="android:layout_width">55dp</item>
        <item name="android:layout_height">55dp</item>
    </style>

    <style name="assosiation_subtitle_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">35dp</item>
        <item name="android:layout_marginBottom">35dp</item>
        <item name="android:fontFamily">@font/inriasansregular</item>
        <item name="android:textColor">@color/design_default_color_on_secondary</item>
        <item name="android:textSize">16pt</item>
    </style>

    <style name="assosiation_subtitle2_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">35dp</item>
        <item name="android:fontFamily">@font/inriasansregular</item>
        <item name="android:textColor">@color/design_default_color_on_secondary</item>
        <item name="android:textSize">16pt</item>
    </style>

    <style name="create_btn">
        <item name="android:textColor">#FFF</item>
        <item name="android:backgroundTint">#000000</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">20dp</item>
    </style>

    <style name="card_checkbox">

        <item name="android:clickable">false</item>
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:background">@drawable/checkbox_selector</item>
        <item name="android:button">@null</item>
    </style>
</resources>